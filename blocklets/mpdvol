filter() {
    sed '3q;d' | cut -c 8-10
}

case $BLOCK_BUTTON in
    3) mpc $password volume 0 | filter ;;  # right click, pause/unpause
    4) mpc $password volume +5 | filter ;;  # scroll up, next
    5) mpc $password volume -5 | filter ;;  # scroll down, previous
    *) mpc $password status | filter ;;
esac

