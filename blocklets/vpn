case $BLOCK_BUTTON in
    1) urxvt -e sudo openvpn ~/airvpn/airvpn.ovpn & disown | echo "Starting VPN... Enter Password";;
    2) urxvt -e sh ~/dotfiles/blocklets/vpntor ;;
    3) urxvt -e sudo pkill openvpn | echo "Stopping VPN... Enter Password";;
esac

vpn() {
	sh ~/dotfiles/blocklets/vpncheck
}

vpn
