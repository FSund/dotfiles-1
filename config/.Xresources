! == Define Font == !
#define FNT Hack
#define STYL
#define SIZ 8

! == Set Params == !
*.font: FNT STYL SIZ
i3bar.height: 20

! == Color Scheme == !
#define xbackground #000000
#define xforeground #eebb77
#define xcolor0 #eebb77 
#define xcolor1 #0099ff 
#define xcolor2 #667788 
#define xcolor3 #886666 
#define xcolor4 #aa9988 
#define xcolor5 #99aabb 
#define xcolor6 #002255 
#define xcolor7 #66ccff 
#define xcolor8 #3388cc 
#define xcolor9 #bb9944 
#define xcolor10 #eebb77 
#define xcolor11 #667788 
#define xcolor12 #eeddcc 
#define xcolor13 #8bc3dd 
#define xcolor14 #eac151 
#define xcolor15 #268ff6 

 *.background: xbackground
 *.foreground: xforeground
 *.color0: xcolor0
 *.color1: xcolor1
 *.color2: xcolor2
 *.color3: xcolor3
 *.color4: xcolor4
 *.color5: xcolor5
 *.color6: xcolor6
 *.color7: xcolor7
 *.color8: xcolor8
 *.color9: xcolor9
 *.color10: xcolor10
 *.color11: xcolor11
 *.color12: xcolor12
 *.color13: xcolor13
 *.color14: xcolor14
 *.color15: xcolor15
*.depth: 32

! == Xft == !
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! == URxvt == !
URxvt.font:	 xft:FNT:size=SIZ
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: 1
URxvt.internalBorder:  2


! == Rofi == !
rofi.padding: 2
rofi.width: 20
rofi.bw: 1
rofi.location: 3
rofi.xoffset: 0
rofi.yoffset: 16
rofi.font: FNT STYL SIZ
rofi.auto-select: true
rofi.opacity: 100
rofi.fullscreen: false
rofi.color-enabled: true
rofi.separator-style: dashed
rofi.color-window: xbackground, xbackground, xbackground
rofi.color-normal: xbackground, xforeground, xbackground, xbackground, xcolor5
rofi.color-active: xbackground, xcolor8, xbackground, xbackground, xcolor5
rofi.color-urgent: xbackground, xcolor0, xbackground, xbackground, xcolor15
