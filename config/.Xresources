! == Define Font == !
#define FNT Hack
#define STYL Bold
#define SIZ 12

! == Set Params == !
*.font: FNT STYL SIZ
i3bar.height: 36

! == Color Scheme == !
#define xbackground #27144D
#define xforeground #66eeff
#define xcolor0 #ffee77
#define xcolor1 #aaffff
#define xcolor2 #ff9966
#define xcolor3 #66eeff
#define xcolor4 #cc6655
#define xcolor5 #66aaff
#define xcolor6 #bb99cc
#define xcolor7 #4477cc
#define xcolor8 #806e69
#define xcolor9 #a072c8
#define xcolor10 #9983c8
#define xcolor11 #8592c8
#define xcolor12 #9fabca
#define xcolor13 #e0b467
#define xcolor14 #59bde0
#define xcolor15 #a3dee0

 *.background: xbackground
 *.foreground: xforeground
 *.color0: xcolor0
 *.color1: xcolor1
 *.color2: xcolor2
 *.color3: xcolor3
 *.color4: xcolor4
 *.color5: xcolor5
 *.color6: xcolor6
 *.color7: xcolor7
 *.color8: xcolor8
 *.color9: xcolor9
 *.color10: xcolor10
 *.color11: xcolor11
 *.color12: xcolor12
 *.color13: xcolor13
 *.color14: xcolor14
 *.color15: xcolor15
*.depth: 32

! == Xft == !
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! == URxvt == !
URxvt.font:	 xft:FNT:STYL:size=SIZ
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: 1

! == Rofi == !
rofi.padding: 15
rofi.width: 20
rofi.bw: 2
rofi.color-enabled: true
rofi.separator-style: solid
rofi.color-window: xbackground, xcolor4, xcolor2
rofi.color-normal: xbackground, xcolor9, xbackground, xbackground, xcolor5
rofi.color-active: xbackground, xcolor1, xbackground xbackground, xcolor0
rofi.color-urgent: xbackground, xcolor4, xbackground, xbackground, xcolor0
