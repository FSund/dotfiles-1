! == Define Font == !
#define FNT Hack
#define STYL
#define SIZ 8

! == Set Params == !
*.font: FNT STYL SIZ
i3bar.height: 20

! == Color Scheme == !
#define xbackground #000000
#define xforeground #0effff
#define xcolor0 #0effff
#define xcolor1 #322ea0
#define xcolor2 #4c30a0
#define xcolor3 #3c41a2
#define xcolor4 #742ea0
#define xcolor5 #5936b3
#define xcolor6 #4666c0
#define xcolor7 #0ac5b5
#define xcolor8 #321d6f
#define xcolor9 #4a44ed
#define xcolor10 #7046ed
#define xcolor11 #565eeb
#define xcolor12 #ac44ed
#define xcolor13 #7f4cff
#define xcolor14 #608cff
#define xcolor15 #0effff

 *.background: xbackground
 *.foreground: xforeground
 *.color0: xcolor0
 *.color1: xcolor1
 *.color2: xcolor2
 *.color3: xcolor3
 *.color4: xcolor4
 *.color5: xcolor5
 *.color6: xcolor6
 *.color7: xcolor7
 *.color8: xcolor8
 *.color9: xcolor9
 *.color10: xcolor10
 *.color11: xcolor11
 *.color12: xcolor12
 *.color13: xcolor13
 *.color14: xcolor14
 *.color15: xcolor15
*.depth: 32

! == Xft == !
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! == URxvt == !
URxvt.font:	 xft:FNT:size=SIZ
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: 1
URxvt.internalBorder:  2


! == Rofi == !
rofi.padding: 2
rofi.width: 20
rofi.bw: 1
rofi.location: 3
rofi.xoffset: 0
rofi.yoffset: 16
rofi.font: FNT STYL SIZ
rofi.auto-select: true
rofi.opacity: 100
rofi.fullscreen: false
rofi.color-enabled: true
rofi.separator-style: dashed
rofi.color-window: xbackground, xbackground, xbackground
rofi.color-normal: xbackground, xforeground, xbackground, xbackground, xcolor5
rofi.color-active: xbackground, xcolor8, xbackground, xbackground, xcolor5
rofi.color-urgent: xbackground, xcolor0, xbackground, xbackground, xcolor15
