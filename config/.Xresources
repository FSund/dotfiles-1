! == Define Font == !
#define FNT Dina
#define STYL
#define SIZ 10

! == Set Params == !
*.font: FNT STYL SIZ
i3bar.height: 20

! == Color Scheme == !
*color0: #171930
*color1: #0b4756
*color2: #361097
*color3: #39dbbd
*color4: #68b421
*color5: #165C91
*color6: #525E95
*color7: #a81b4d
*color8: #222548
*color9: #1590af
*color10: #5619f1
*color11: #4dfefe
*color12: #98fe30
*color13: #2293e8
*color14: #7586d5
*color15: #fe2874
*.depth: 32

! == Xft == !
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! == URxvt == !
URxvt.font:	 xft:FNT:size=SIZ
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: 1
URxvt.internalBorder: 10
URxvt.depth: 32
URxvt.background: rgba:1111/1111/3333/CCCC
URxvt.cursorUnderline: true
URxvt.cursorColor: color5

! == Rofi == !
rofi.padding: 10
rofi.width: 25
rofi.bw: 1
rofi.location: 6
!rofi.xoffset: 0
rofi.yoffset: -50
rofi.font: FNT STYL SIZ
rofi.auto-select: true
rofi.opacity: 90
rofi.fullscreen: false
rofi.color-enabled: true
rofi.separator-style: dashed
rofi.color-window: xbackground, xcolor1, xbackground
rofi.color-normal: xbackground, xforeground, xbackground, xbackground, xcolor5
rofi.color-active: xbackground, xcolor8, xbackground, xbackground, xcolor5
rofi.color-urgent: xbackground, xcolor0, xbackground, xbackground, xcolor15
