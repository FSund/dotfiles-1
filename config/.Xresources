! == Define Font == !
#define FNT Dina
#define STYL
#define SIZ 10

! == Set Params == !
*.font: FNT STYL SIZ
i3bar.height: 20

! == Color Scheme == !
#define xbackground #150021
#define xforeground #4E7971
#define xcolor0 #171930
#define xcolor1 #0b4756
#define xcolor2 #361097
#define xcolor3 #39dbbd
#define xcolor4 #68b421
#define xcolor5 #165C91
#define xcolor6 #525E95
#define xcolor7 #a81b4d
#define xcolor8 #222548
#define xcolor9 #1590af
#define xcolor10 #5619f1
#define xcolor11 #4dfefe
#define xcolor12 #98fe30
#define xcolor13 #2293e8
#define xcolor14 #7586d5
#define xcolor15 #fe2874

 *.background: xbackground
 *.foreground: xforeground
 *.color0: xcolor0
 *.color1: xcolor1
 *.color2: xcolor2
 *.color3: xcolor3
 *.color4: xcolor4
 *.color5: xcolor5
 *.color6: xcolor6
 *.color7: xcolor7
 *.color8: xcolor8
 *.color9: xcolor9
 *.color10: xcolor10
 *.color11: xcolor11
 *.color12: xcolor12
 *.color13: xcolor13
 *.color14: xcolor14
 *.color15: xcolor15
*.depth: 32

! == Xft == !
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! == URxvt == !
URxvt.font:	 xft:FNT:size=SIZ
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: 1
URxvt.internalBorder: 10
URxvt.depth: 32
URxvt.background: rgba:1111/1111/3333/CCCC
URxvt.cursorUnderline: true
URxvt.cursorColor: color5

! == Rofi == !
rofi.padding: 10
rofi.width: 25
rofi.bw: 1
rofi.location: 6
!rofi.xoffset: 0
rofi.yoffset: -50
rofi.font: FNT STYL SIZ
rofi.auto-select: true
rofi.opacity: 90
rofi.fullscreen: false
rofi.color-enabled: true
rofi.separator-style: dashed
rofi.color-window: xbackground, xcolor1, xbackground
rofi.color-normal: xbackground, xforeground, xbackground, xbackground, xcolor5
rofi.color-active: xbackground, xcolor8, xbackground, xbackground, xcolor5
rofi.color-urgent: xbackground, xcolor0, xbackground, xbackground, xcolor15
