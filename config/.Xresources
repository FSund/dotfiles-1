! == Define Font == !
#define FNT Hack
#define STYL
#define SIZ 10

! == Set Params == !
*.font: FNT STYL SIZ
i3bar.height: 20

! == Color Scheme == !
#define xbackground #1C1E20
#define xforeground #55A9C8
#define xcolor0 #66dd33
#define xcolor1 #9944cc
#define xcolor2 #ee4488
#define xcolor3 #4433bb
#define xcolor4 #33ccaa
#define xcolor5 #ee4488
#define xcolor6 #553355
#define xcolor7 #cc3322
#define xcolor8 #557733
#define xcolor9 #336655
#define xcolor10 #dd55cc
#define xcolor11 #883344
#define xcolor12 #883333
#define xcolor13 #A1C864
#define xcolor14 #8B7BC8
#define xcolor15 #61A053

 *.background: xbackground
 *.foreground: xforeground
 *.color0: xcolor0
 *.color1: xcolor1
 *.color2: xcolor2
 *.color3: xcolor3
 *.color4: xcolor4
 *.color5: xcolor5
 *.color6: xcolor6
 *.color7: xcolor7
 *.color8: xcolor8
 *.color9: xcolor9
 *.color10: xcolor10
 *.color11: xcolor11
 *.color12: xcolor12
 *.color13: xcolor13
 *.color14: xcolor14
 *.color15: xcolor15
*.depth: 32

! == Xft == !
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! == URxvt == !
URxvt.font:	 xft:FNT:size=SIZ
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.cursorBlink: 1

! == Rofi == !
rofi.padding: 10
rofi.width: 20
rofi.bw: 1
rofi.color-enabled: true
rofi.separator-style: dashed
rofi.color-window: xbackground, xbackground, xcolor2
rofi.color-normal: xbackground, xcolor9, xbackground, xbackground, xcolor5
rofi.color-active: xbackground, xcolor1, xbackground xbackground, xcolor0
rofi.color-urgent: xbackground, xcolor4, xbackground, xbackground, xcolor0
